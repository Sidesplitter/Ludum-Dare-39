        <arguments>
          <argument>
            <kind>1</kind>
            <string>if (!instance_exists(obj_golfball)) {
    exit;
}

image_angle=direction;
image_index = 0;
image_speed = 0;

if (charging) {
    if (charge&lt;100)
    charge += 1;
}

if (shooting) {
    if (abs(clubAngle&lt;=1)) {
        obj_golfball.direction = direction + 90;
        obj_golfball.speed = charge / 4;
        global.golfswings += 1;
        shooting = false;
        active = false;
        waiting = true;
    }
    
} else {
    if (!waiting &amp;&amp; active &amp;&amp; !incart &amp;&amp; !driving &amp;&amp; obj_golfball.speed &lt;= 0.1) {
        if (gotoball) {
            move_towards_point(obj_golfball.x, obj_golfball.y, 3);
            if (point_distance(x,y,obj_golfball.x, obj_golfball.y)&lt;50) {
                gotoball = false;
            }
        } else {
        
            x += ((obj_golfball.x - lengthdir_x(62, image_angle)) - x)/10;
            y += ((obj_golfball.y - lengthdir_y(62, image_angle)) - y)/10;
            directionTarget = point_direction(obj_golfball.x,obj_golfball.y,mouse_x,mouse_y)-90;
            speed = 0;
            direction+=(angle_diff(direction,directionTarget))/10.0;
        }
    }
}

clubAngle+=(clubAngleTarget-clubAngle)/10.0;

if (incart) {
    x = obj_golfcart.x;
    y = obj_golfcart.y;
}
if (!active) {
    if (obj_golfball.speed &lt;= 0.1) {
        if (incart &amp;&amp; point_distance(x,y,obj_golfball.x+150,y)&lt;30) {
            active = true;
            driving = false;
            incart = false;
            obj_golfcart.trump = false;
            waiting = false;
        } else {
            driving = true;
            
            show_debug_message("gotoball");
            gotoball = true;
            if (!incart) {
                move_towards_point(obj_golfcart.x, obj_golfcart.y, 3);
                if (distance_to_object(obj_golfcart)&lt;2) {
                    incart = true;
                    obj_golfcart.trump = true;
                }
            }
        }
    } else {
        active = false;
    }
}
</string>
          </argument>
        </arguments>
      </action>
